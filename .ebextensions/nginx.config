files:
  /etc/nginx/conf.d/redirect.conf:
    mode: "000644"
    owner: root
    group: root
    content: |
      # Redirect HTTP To HTTPS

      server {
        listen 81;
        rewrite ^ https://$host$request_uri permanent;
      }
      server {
        server_name portfolio-flask-backend.us-west-2.elasticbeanstalk.com;
        return 301 api-flask.uhucream-takashi.pictures$request_uri;
      }
