{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "flask-src",
      "host": {
        "sourcePath": "/var/app/current/app"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "flask",
      "image": "python:3.8",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 5000,
          "containerPort": 5000
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "flask-src",
          "containerPath": "/usr/src/app",
          "readOnly": true
        }
      ],
      "workingDirectory": "/usr/src/app"
    }
  ]
}